<!--pages/list/index.wxml-->
<import src="/pages/template/list-template.wxml"></import>
<view>
  <swiper catchtap="carouselToDetail" indicator-dots="true" indicator-color="yellowgreen" indicator-active-color="pink">
    <swiper-item>
      <image data-index="0" src="/images/ai.jpg"></image>
    </swiper-item>
    <swiper-item>
      <image data-index="1" src="{{userInfo.avatarUrl}}"></image>
    </swiper-item>
  </swiper>
  <block wx:for="{{listArr}}" wx:key='{{index}}'>
    <view catchtap="toDetail" data-index='{{index}}'>
      <template is="listTmp" data="{{...item}}"></template>
    </view>
  </block>
</view>
